{
    "name": "gokure/hyperf-http-logger",
    "description": "Log HTTP requests and responses for Hyperf.",
    "keywords": ["hyperf", "logger", "http-logger", "request", "request-logger"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Gang Wu",
            "email": "gokure@gmail.com"
        }
    ],
    "require": {
        "php": ">= 7.2",
        "ext-swoole": ">= 4.4",
        "psr/log": "^1.0",
        "psr/container": "^1.0",
        "hyperf/contract": "^1.0|^2.0",
        "hyperf/di": "^1.0|^2.0",
        "hyperf/http-server": "^1.0|^2.0",
        "hyperf/utils": "^1.0|^2.0",
        "monolog/monolog": "^2.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0.0",
        "hyperf/testing": "^1.0|^2.0",
        "roave/security-advisories": "dev-master",
        "swoole/ide-helper": "^4.5"
    },
    "autoload": {
        "psr-4": {
            "Gokure\\HttpLogger\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "hyperf": {
            "config": "Gokure\\HttpLogger\\ConfigProvider"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
